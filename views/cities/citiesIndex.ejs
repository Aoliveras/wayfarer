
    <div class="container">
        <div class="col s4">
                <% if (success && loggedIn) { %>
                <% for(let i=0; i < payload.length; i++) { %>
                        <div class="card">
                            <div class="card-content">
                                <a href="/cities/<%= payload[i]._id %>">
                                    <h2><%= payload[i].name %></h2>
                                </a>
                            <img src="<%= payload[i].imageUrl%>" alt="city image" style="width: 250px;">
                        </div> 
                        </div>
                <% } %>
                <% } else { %>
                    <h1>Uh, oh!</h1>
                    <p>You must be signed in to view this information. Please <a href="/users/login">Login</a> or <a href="/users/signup">Sign Up</a>.</p>
                    <% } %>
        </div>
    </div>
